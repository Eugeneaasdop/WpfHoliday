<Window x:Class="Wpf_Holiday.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_Holiday"
        mc:Ignorable="d"
        Title="Holiday" Height="750" Width="1100">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="0.8*"/>
        </Grid.RowDefinitions>
        <StackPanel Background="#E34234" Grid.Row="0"/>
        <StackPanel Background="#E34234" Grid.Row="2"/>
        <Frame Grid.Row="0" Name="FrmGlav" Background="Snow" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
